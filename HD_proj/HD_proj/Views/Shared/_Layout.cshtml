<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Phần mềm quản lý CCKD</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/lib/addon_theme/css/sb-admin-2.css" rel="stylesheet">
    <link href="~/lib/addon_theme/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <link href="~/lib/addon_theme/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
 
    <link href="~/lib/calendar/lib/main.css" rel="stylesheet" />
</head>
<body id="page-top" class="bg-gradient-primary">
    <div id="wrapper">
        <partial name="_LoginPartial" />
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                <partial name="_NavPartial" />
                @RenderBody()
            </div>
            <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy;</span>
                    </div>
                </div>
            </footer>
        </div>

        <a class="scroll-to-top rounded" href="#page-top">
            <i class="fas fa-angle-up"></i>
        </a>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/addon_theme/vendor/jquery-easing/jquery.easing.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/lib/addon_theme/js/sb-admin-2.js"></script>
    <script src="~/lib/addon_theme/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/lib/addon_theme/vendor/datatables/dataTables.bootstrap4.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    <script src="~/lib/calendar/lib/moment.js"></script>
    <script src="~/lib/calendar/lib/main.js"></script>
    <script src="~/lib/calendar/lib/locales-all.js"></script>
  
    @RenderSection("Scripts", required: false)
</body>
</html>
